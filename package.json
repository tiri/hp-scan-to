    "codecov": "^3.8.3",
    "eslint": "^8.15.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.4",
    "yarn": "^1.22.18",
    "@istanbuljs/nyc-config-typescript": "^1.0.2"
  },
